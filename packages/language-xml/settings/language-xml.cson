'.text.xml':
  'editor':
    'commentStart': '<!-- '
    'commentEnd': ' -->'
    'commentDelimiters':
      'block': ['<!--', '-->']
    'foldEndPattern': '^\\s*(</[^>]+>|[/%]>|-->)\\s*$'
    'increaseIndentPattern': '^\\s*<(([^!/?]|%)(?!.+?([/%]>|</.+?>))|[%!]--\\s*$)'
    'decreaseIndentPattern': '^\\s*(</[^>]+>|-->|--%>)'
