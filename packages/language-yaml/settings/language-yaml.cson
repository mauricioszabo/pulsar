'.source.yaml':
  'editor':
    'autoIndentOnPaste': false
    'commentStart': '# '
    'commentDelimiters':
      'line': '#'
    'foldEndPattern': '^\\s*$|^\\s*\\}|^\\s*\\]|^\\s*\\)'
    'increaseIndentPattern': '^\\s*.*(:|-) ?(&\\w+)?(\\{[^}"\']*|\\([^)"\']*)?$'
    'decreaseIndentPattern': '^\\s+\\}$'
    'tabType': 'soft'
