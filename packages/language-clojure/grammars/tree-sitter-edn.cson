name: 'Clojure EDN'
scopeName: 'source.edn'
type: 'modern-tree-sitter'
injectionRegExp: '^source-clojure-edn$'
parser: 'tree-sitter-clojure'

fileTypes: [
]
treeSitter:
  grammar: 'ts/grammar.wasm'
  highlightsQuery: [
    'ts/edn-only-highlights.scm'
    'ts/edn-highlights.scm'
  ]
  foldsQuery: 'ts/folds.scm'
